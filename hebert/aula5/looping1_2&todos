#include <stdio.h>

int main()
{
  int i;
  i=10;
  while (i<14){
      printf("%d ",i);
      i++;
  }
  return 0;
}

        2

#include <stdio.h>

int main()
{
  int i;
  i=4;
  while (i>-1){
      printf("%d ",i);
      i--;
  }
  return 0;
}


